{
  "name": "pt_client",
  "cnname": "期权定价工具管理客户端",
  "version": "1.1.0",
  "description": "An electron-vue project",
  "scripts": {
    "build": "cd app && rm -rf node_modules && cd .. && rm -rf node_modules && npm i && node tasks/release.js",
    "build:clean": "cross-env PLATFORM_TARGET=clean node tasks/release.js",
    "build:darwin": "cross-env PLATFORM_TARGET=darwin PACK=no node tasks/release.js",
    "build:win32": "cross-env PLATFORM_TARGET=win32 node tasks/release.js",
    "build:exe": "npm run build:win32 && node tasks/buildexe.js",
    "dev": "node tasks/runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.renderer.config.js",
    "postinstall": "cd app && npm i"
  },
  "author": "ComStar",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "copy-webpack-plugin": "^4.3.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "devtron": "^1.1.0",
    "electron": "^1.7.8",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^10.1.0",
    "electron-rebuild": "^1.1.3",
    "electron-winstaller": "^2.6.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.16.1",
    "json-loader": "^0.5.4",
    "node-loader": "^0.6.0",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "script-loader": "^0.7.2",
    "style-loader": "^0.19.0",
    "svg-sprite-loader": "^3.5.2",
    "tree-kill": "^1.2.0",
    "url-loader": "^0.6.2",
    "vue-hot-reload-api": "^2.2.4",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {}
}
